-- Test 05: Expected parse errors (skip in automated tests)
-- These queries SHOULD fail with parse errors
.load ./build/graphqlite.so

-- Invalid syntax
SELECT cypher('INVALID SYNTAX');

-- Missing closing parenthesis
SELECT cypher('CREATE (n:Person');

-- Invalid property syntax
SELECT cypher('CREATE (n:Person {name "Alice"})');

-- Empty query
SELECT cypher('');

-- Missing colon
SELECT cypher('CREATE (n Person)');

-- Invalid property value
SELECT cypher('CREATE (n:Test {prop: })');

-- Unclosed string
SELECT cypher('CREATE (n:Test {name: "unclosed})');